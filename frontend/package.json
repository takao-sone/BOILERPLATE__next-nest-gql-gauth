    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "babel-plugin-import": "^1.13.6",
    "encoding": "^0.1.13",
    "eslint": "8.24.0",
    "eslint-config-next": "^13.0.6",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^16.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.6.0",
    "typescript": "^4.8.4"
