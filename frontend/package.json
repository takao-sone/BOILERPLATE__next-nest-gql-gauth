    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.1",
